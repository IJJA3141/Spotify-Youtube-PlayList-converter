{
  "name": "spotify-youtube-playlist-converter",
  "version": "0.0.0",
  "description": "Spotify to youtube playlist converter",
  "scripts": {
    "cpf": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command cpf ./src/ ./build/ '*.css','*.html'",
    "rm": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command rm ./build/ -Recurse",
    "build": "npm run rm && npm run cpf && tsc -p ./src/",
    "electron": "electron ./build/main.js",
    "start": "npm run build && npm run electron"
  },
  "author": "IJJA3141",
  "license": "ISC",
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/jest": "^29.5.3",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1"
  },
  "Dependencies": {
    "electron": "^25.3.0"
  },
  "dependencies": {
    "dotenv": "^16.3.1"
  }
}
